a:Button("Summon!", function()
    if summon then
        start(summon)
    end
end)

function start(summon)
    chatModule:Chat(summon)
    local yes = Notification.new("success", "Summons", "Successfully summoned the selected choice.")
    yes:deleteTimeout(2)
end

function spawnlocation(ok)
    print("hi")
    if plyr.character then
        plyr.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.Baseplate.CFrame
        local yes = Notification.new("success", "Teleports", "Successfully teleported to spawn.")
        yes:deleteTimeout(2)
    end
end

function mines()
    if plyr.character then
        plyr.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Mines1.Part.CFrame
        local yes = Notification.new("success", "Teleports", "Successfully teleported to the Mines.")
        yes:deleteTimeout(2)
    end
end

function minigames()
    if plyr.character then
        plyr.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.ArchiverRoom.Part.CFrame
        local yes = Notification.new("success", "Teleports", "Successfully teleported to the Minigames.")
        yes:deleteTimeout(2)
    end
end

function archiver()
    if plyr.Character then
        plyr.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Archiver.HumanoidRootPart.CFrame
        local yes = Notification.new("success", "Teleports", "Successfully teleported to the Archiver.")
        yes:deleteTimeout(2)
    end
end

function praefactus()
    if plyr.Character then
        plyr.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Model.Praefactus.HumanoidRootPart.CFrame
        local yes = Notification.new("success", "Teleports", "Successfully teleported to Praefactus.")
        yes:deleteTimeout(2)
    end
end

function skip()
    chatModule:Chat("/skip")
    local yes = Notification.new("success", "Client", "Successfully skipped the biome.")
    yes:deleteTimeout(2)
    if getgenv().skipcooldown then
        local cooldown = Notification.new("message", "Cooldown", "Time remaining: 30 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 29 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 28 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 27 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 26 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 25 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 24 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 23 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 22 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 21 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 20 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 19 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 18 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 17 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 16 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 15 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 14 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 13 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 12 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 11 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 10 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 9 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 8 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 7 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 6 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 5 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 4 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 3 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 2 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: 1 seconds.")
        wait(1)
        cooldown:changeBody("Time remaining: Finished.")
        wait(1.5)
        cooldown:delete()


    end
end

b:Button("Spawn", function()
    local ok = true;
    spawnlocation()
end)

b:Bind("Bind",Enum.KeyCode.E,function()
    skip()
end)

b:Button("Mines", function()
    mines()
end)

b:Button("Minigames", function()
    minigames()
end)

b:Button("Archiver", function()
    archiver()
end)

b:Button("Praefactus", function()
    praefactus()
end)

c:Button("Skip Biome", function()
    skip()
end)

c:DestroyGui()

d:Toggle("Cooldown notifications", function(bool)
    getgenv().skipcooldown = bool
end)
